//목록화면
<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="utf-8">
  <title>Docs url</title>
  <link rel="stylesheet" href="/static/index.css">
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
</head>
<body>
  <div class="wrap">
    <h1>목 록 화 면</h1>
    <a href="/input" class="button">작성</a>
    <a href="/delete" class="button">삭제</a>
    <table class="table type10">
      <thead>
        <tr>
          <th>제목</td>
        </tr>
      </thead>
      <tbody>
        <%data.forEach(item => {%>
          <tr>
            <td><a href="/open/<%=item.id %>"><%=item.title %></a></td>
          </tr>
        <%})%>
      </tbody>
    </table>
  </div>
</body>
</html>